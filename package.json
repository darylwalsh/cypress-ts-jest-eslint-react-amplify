{
  "name": "cypress-ts-jest-eslint-react-amplify",
  "version": "0.1.7",
  "description": "cypress typescript jest react amplify",
  "scripts": {
    "commit": "git-cz",
    "test": "...",
    "build": "next build",
    "dev": "next",
    "lint": "eslint *.ts*",
    "start": "next start",
    "type-check": "tsc",
    "styleguide:build": "...",
    "prettier:check": "prettier --check 'src/**/*.{js,ts,tsx,js,mdx}'",
    "validate": "run-s test lint prettier:check",
    "prerelease": "git-authors-cli && git add .",
    "release": "standard-version -a",
    "postrelease": "run-s release:*",
    "release:tags": "git push --follow-tags origin master",
    "release:github": "conventional-github-releaser -p angular",
    "ci:validate": "rm -rf node_modules && npm ci && npm run validate",
    "prepublishOnly": "npm run ci:validate && npm run build"
  },
  "dependencies": {
    "next": "latest",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/node": "^11.13.9",
    "@types/react": "^16.8.15",
    "@types/react-dom": "^16.0.11",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "babel-eslint": "^10.0.3",
    "chalk": "^2.4.2",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.23",
    "conventional-github-releaser": "^3.1.3",
    "cz-adapter-eslint": "^0.3.0",
    "eslint": "^6.5.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-eslint-plugin": "^2.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-utils": "^1.4.2",
    "git-authors-cli": "^1.0.18",
    "husky": "^3.0.7",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.0",
    "node-fetch": "^2.6.0",
    "now": "^16.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "randomstring": "^1.1.5",
    "rimraf": "^3.0.0",
    "scaffdog": "^0.1.2",
    "standard-version": "^7.0.0",
    "thread-loader": "^2.1.3",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.2",
    "ts-node": "^8.4.1",
    "typescript": "3.5.2",
    "webpack": "^4.41.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "tsc && npm run lint"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "git add"
    ],
    "services/client/src/**/*.{js,jsx,css,ts,tsx,md}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "contributors": [
    "Daryl <daryl@blpc.us>"
  ],
  "license": "ISC"
}
