{
  "name": "cypress-ts-jest-eslint-react-amplify",
  "version": "0.1.13",
  "description": "cypress typescript jest react amplify",
  "scripts": {
    "build": "next build && next export -o build/",
    "dev": "next",
    "lint": "eslint src/**/*.ts*  src/*.ts*",
    "mock:api": "yarn kill-amplify-mock-api && amplify mock api",
    "kill-amplify-mock-api": "kill-port 20002 && kill-port 20003",
    "reset-mock": "yarn kill-amplify-mock-api && amplify mock api",
    "start": "next start",
    "test:static": "yarn lint && yarn tsc",
    "test:jest": "yarn jest",
    "test:e2e": "/bin/bash ./scripts/e2e.sh",
    "test:e2e:dev": "yarn kill-amplify-mock-api && (amplify mock api &) && wait-on http-get://localhost:20002 && kill-port 3000 && (yarn dev &) && wait-on http-get://localhost:3000 && PORT=3000 cypress open --env PORT=3000",
    "test": "npm test:static && npm test:jest",
    "commit": "git-cz",
    "type-check": "tsc",
    "styleguide:build": "...",
    "prettier:check": "prettier --check 'src/**/*.{js,ts,tsx,js,mdx}'",
    "validate": "run-s test lint prettier:check",
    "prerelease": "git-authors-cli && git add .",
    "release": "standard-version -a",
    "postrelease": "run-s release:*",
    "release:tags": "git push --follow-tags origin master",
    "release:github": "conventional-github-releaser -p angular",
    "ci:validate": "rm -rf node_modules && npm ci && npm run validate",
    "prepublishOnly": "npm run ci:validate && npm run build"
  },
  "dependencies": {
    "@aws-amplify/api": "^1.0.42",
    "@aws-amplify/pubsub": "^1.1.0",
    "immer": "^3.2.0",
    "lodash.memoize": "^4.1.2",
    "nanoid": "^2.0.3",
    "next": "^9.0.3",
    "next-offline": "^4.0.3",
    "react": "^16.9.0",
    "react-animated-css": "^1.2.1",
    "react-dom": "^16.9.0",
    "react-intersection-observer": "^8.24.1",
    "react-native-web": "^0.11.6"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@testing-library/cypress": "^4.1.0",
    "@testing-library/react": "^9.1.1",
    "@types/jest": "^24.0.17",
    "@types/jest-in-case": "^1.0.1",
    "@types/lodash.memoize": "^4.1.6",
    "@types/nanoid": "^2.0.0",
    "@types/node": "^12.7.1",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.0.11",
    "@types/react-native-web": "npm:@types/react-native",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.4",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.8.0",
    "chalk": "^2.4.2",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.23",
    "conventional-github-releaser": "^3.1.3",
    "cz-adapter-eslint": "^0.3.0",
    "cypress": "^3.4.1",
    "eslint": "^6.1.0",
    "eslint-plugin-cypress": "^2.6.1",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-eslint-plugin": "^2.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-utils": "^1.4.2",
    "git-authors-cli": "^1.0.18",
    "husky": "^3.0.4",
    "jest": "^24.8.0",
    "lint-staged": "^9.4.0",
    "node-fetch": "^2.6.0",
    "now": "^16.2.0",
    "npm-run-all": "^4.1.5",
    "jest-in-case": "^1.0.2",
    "kill-port": "^1.5.1",
    "prettier": "^1.18.2",
    "randomstring": "^1.1.5",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.2",
    "scaffdog": "^0.1.2",
    "standard-version": "^8.0.1",
    "thread-loader": "^2.1.3",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.5.3",
    "wait-on": "^3.3.0",
    "webpack": "^4.41.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "tsc && npm run lint"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "git add"
    ],
    "services/client/src/**/*.{js,jsx,css,ts,tsx,md}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "contributors": [
    "Daryl <daryl@blpc.us>"
  ],
  "license": "ISC"
}
